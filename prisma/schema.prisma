// para ver los modelos despues de npx prisma studio- entrar a http://localhost:5555
//npx prisma migrate dev --name init
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Afiliado {
  id          String   @id
  name        String   @db.VarChar(50)
  email       String   @db.VarChar(200)
  password    String   @db.VarChar(200)
  imageUrl    String   @db.VarChar(800)
  dni         String   @db.VarChar(8)
  phone       String   @db.VarChar(13)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Operador {
  id           String   @id
  name         String   @db.VarChar(50)
  email        String   @db.VarChar(200)
  password     String   @db.VarChar(200)
  imageUrl     String   @db.VarChar(800)
  phone        String   @db.VarChar(13)
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  dataTime     DateTime
  area         String   @db.VarChar(150)
  numeroOperador String @db.VarChar(5)
  privilegios  Json
}

model Prestador {
  id           String   @id
  name         String   @db.VarChar(50)
  email        String   @db.VarChar(200)
  password     String   @db.VarChar(200)
  phone        String   @db.VarChar(13)
  imageUrl     String   @db.VarChar(800)
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  dataTime     DateTime
  matricula    String   @db.VarChar(5)
  especialidad String   @db.VarChar(150)
  puntuacion   Int
}